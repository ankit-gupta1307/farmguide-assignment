@import '_normalize.styl'
@import '_typography.styl'
@import '_animations.styl'
@import 'bootstrap.css'
body
  background #cecece;

.triangle
  width: 0; 
  height: 0; 
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-bottom: 7px solid white;
  position: relative;
  left: 310px;
  bottom: -10px;

.arrow-left 
  width: 0; 
  height: 0; 
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent; 
  border-right: 7px solid #fff; 
  position: relative;
  left: 13px;
  top: 30px;

.filterCont
  width: 35px;
  height: 35px;
  position: relative;
  top: 30px;
  left: -10px;
  padding-top: 6px;
  padding-right: 6px;
  border: 1.5px solid #cecece;

.checkboxes
  padding-left: 5px;
  position: relative;
  bottom: 3px;

.circle1
  position: relative;
  top: -20px;
  left: 20px;
  height: 20px;
  width: 40px;
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
  background: brown;
  

.circle2
  position: relative;
  top: -40px;
  left: 140px;
  height: 20px;
  width: 40px;
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
  background: brown;
  

.circle3
  position: relative;
  top: 110px;
  left: 20px;
  height: 20px;
  width: 40px;
  border-bottom-left-radius: 20px;
  border-bottom-right-radius: 20px;
  background: brown;
  

.circle4
  position: relative;
  top: 90px;
  left: 140px;
  height: 20px;
  width: 40px;
  border-bottom-left-radius: 20px;
  border-bottom-right-radius: 20px;
  background: brown;

.skyblue
  background: skyblue !important;

.shown 
  position: absolute;
  top: -40px;
  left: 10px;
  width: 240px; 
  background: #000; 
  z-index: 1000; 
  display: block;

.genPad 
  textAlign: left;
  paddingLeft: 0;

.proTeam 
  textAlign: left; 
  paddingLeft: 5px; 
  fontSize: 14px;

.dropdownName
  textAlign: left; 
  paddingLeft: 0; 
  color: #000; 
  fontSize: 14px;

.hidden 
  display: none;

.filterShowing
    position: absolute;
    top: 60px; 
    right: 80px;
    width: 350px; 
    background: #000;
    z-index: 100; 
    display: block;

.block
  display: block;



  
  

